cargo:rustc-cfg=feature="ntdef"
cargo:rustc-cfg=feature="limits"
cargo:rustc-cfg=feature="ntstatus"
cargo:rustc-cfg=feature="winnt"
cargo:rustc-cfg=feature="excpt"
cargo:rustc-cfg=feature="minwinbase"
